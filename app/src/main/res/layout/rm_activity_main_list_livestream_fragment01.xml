<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Xong.MainActivity_List_LiveStream.Fragment.MainActivity_List_LiveStream_Fragment01">

    <LinearLayout
        android:id="@+id/rm_activity_main_list_livestream_fragment01_ln_01"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:gravity="center">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/rm_activity_main_list_livestream_fragment01_cl_01"
            android:layout_width="match_parent"
            android:layout_height="160dp">

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/rm_activity_main_list_livestream_fragment01_rv_01"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

            <me.relex.circleindicator.CircleIndicator2
                android:id="@+id/rm_activity_main_list_livestream_fragment01_ci_01"
                android:layout_width="match_parent"
                android:layout_height="15dp"
                android:layout_marginBottom="16dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:tabIndicatorColor="@color/black"
                app:tabSelectedTextColor="@android:color/white" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </LinearLayout>

</FrameLayout>